/*----------------------------------
  Media Queries breakpoints
# Define breakpoints for Media Queries
----------------------------------*/

@custom-media --mobile-viewport (max-width: 767.98px);
@custom-media --tablet-viewport (min-width: 768px) and (max-width: 1023.98px);
@custom-media --desktop-viewport (min-width: 1024px);
@custom-media --large_desktop-viewport (min-width: 1200px);
@custom-media --mobile_tablet-viewport (max-width: 1023.98px);
